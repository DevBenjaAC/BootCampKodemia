//if else + if else

let nota = 200;
if (nota === 5) {
  console.log('En hora Buena , haz obtenido un sobresaliente');
}else  if (nota === 4) {
  console.log('Buen trabajao pero podrias haberlo hecho mejor');
}else if (nota === 3) {
  console.log('Haz obtenido un suficiente');
}else if (nota === 2) {
  console.log('No haz aprobado por poco');
}else if (nota === 1) {
  console.log('No haz estudiado nada, trabaja un poquito mas para la proxima');
}else {
  console.log('Error, introduce una nota entre el 1 y el 5');
}